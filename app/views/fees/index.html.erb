<h1>Court fees</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fees.each do |fee| %>
      <tr>
        <td><%= link_to fee.title, fee %></td>
        <td><%= fee.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>
