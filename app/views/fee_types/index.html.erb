<h1>Listing fee_types</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fee_types.each do |fee_type| %>
      <tr>
        <td><%= fee_type.title %></td>
        <td><%= fee_type.amount %></td>
        <td><%= link_to 'Show', fee_type %></td>
        <td><%= link_to 'Edit', edit_fee_type_path(fee_type) %></td>
        <td><%= link_to 'Destroy', fee_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fee type', new_fee_type_path %>
